{
  "name": "@bfmatei/flexiby-ui",
  "version": "0.0.1",
  "description": "Flexiby UI",
  "author": "Bogdan Matei <bogdan@matei.dev>",
  "scripts": {
    "start": "npm run serve:dev",
    "ng": "ng",
    "serve:local": "ng serve --configuration=local",
    "serve:dev": "ng serve",
    "build:dev": "ng build",
    "serve:integration": "ng serve --configuration=integration",
    "build:integration": "ng build --configuration=integration",
    "serve:master": "ng serve --configuration=master",
    "build:master": "ng build --configuration=master",
    "build:master:stats": "ng build --configuration=master --statsJson=true",
    "serve:ci": "ng serve --configuration=ci",
    "build:ci": "ng build --configuration=ci",
    "build:analyze:stats": "webpack-bundle-analyzer --no-open ./dist/app/stats-es2015.json",
    "test": "ng test",
    "test:ci": "ng test --configuration=ci",
    "lint:app": "npm run lint:ts && npm run lint:scss",
    "lint:ts": "eslint \"src/**/*.ts\"",
    "lint:scss": "stylelint \"src/**/*.scss\" -s scss",
    "lint:e2e": "tslint -p e2e/tsconfig.e2e.json",
    "e2e": "ng e2e",
    "prettify": "prettier --write \"./**/*.{js,ts,json,scss,html,yaml}\""
  },
  "dependencies": {
    "@angular/animations": "9.0.0",
    "@angular/common": "9.0.0",
    "@angular/compiler": "9.0.0",
    "@angular/core": "9.0.0",
    "@angular/forms": "9.0.0",
    "@angular/platform-browser": "9.0.0",
    "@angular/platform-browser-dynamic": "9.0.0",
    "@angular/router": "9.0.0",
    "@ngrx/effects": "9.0.0-beta.0",
    "@ngrx/store": "9.0.0-beta.0",
    "@ngrx/store-devtools": "9.0.0-beta.0",
    "rxjs": "6.5.4",
    "tslib": "1.10.0",
    "uuid": "3.4.0",
    "zone.js": "0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.900.1",
    "@angular/cli": "9.0.1",
    "@angular/compiler-cli": "9.0.0",
    "@angular/language-service": "9.0.0",
    "@bmatei/eslint-config": "1.0.12",
    "@bmatei/stylelint-config": "1.0.6",
    "@types/node": "13.7.0",
    "@types/jasmine": "3.5.3",
    "@types/jasminewd2": "2.0.8",
    "@types/uuid": "3.4.7",
    "@typescript-eslint/eslint-plugin": "2.19.0",
    "@typescript-eslint/eslint-plugin-tslint": "2.19.0",
    "@typescript-eslint/parser": "2.19.0",
    "codelyzer": "5.2.1",
    "eslint": "6.8.0",
    "husky": "4.2.1",
    "jasmine-core": "3.5.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "4.4.1",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "2.1.1",
    "karma-jasmine": "3.1.0",
    "karma-jasmine-html-reporter": "1.5.2",
    "lint-staged": "10.0.7",
    "prettier": "1.19.1",
    "protractor": "6.0.0",
    "stylelint": "13.0.0",
    "ts-node": "8.6.2",
    "tslint": "6.0.0",
    "typescript": "3.7.5",
    "typescript-tslint-plugin": "0.5.5",
    "webpack-bundle-analyzer": "3.6.0"
  }
}
